@inherits Core.Web.Components.CanvasManagerBase

<div class="canvases">
    @foreach (var (name, options) in _names)
    {
        <div id="@(name)Container" class="canvasContainer @(options.Primary ? "primary" : "")" hidden="@options.Hidden">
            <BECanvas @ref="_canvases[name]" />
        </div>
    }
</div> 